{
  "name": "@newrelic/proxy",
  "version": "2.0.0",
  "description": "An HTTP proxy written with Node.js (think Squid)",
  "main": "proxy.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "start": "node ./bin/proxy.js"
  },
  "bin": "./bin/proxy.js",
  "keywords": [
    "http",
    "https",
    "proxy",
    "connect",
    "tunnel",
    "squid",
    "privoxy",
    "apache",
    "mod_proxy",
    "via",
    "x-forwarded-for"
  ],
  "author": "Nathan Rajlich <nathan@tootallnate.net> (http://n8.io/)",
  "contributors": [
    {
      "name": "Peter Svetlichny",
      "email": "psvetlichny@newrelic.com",
      "web": "https://newrelic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/newrelic/proxy.git"
  },
  "license": "MIT",
  "dependencies": {
    "args": "^5.0.1",
    "basic-auth-parser": "0.0.2",
    "debug": "^3.2.7"
  },
  "devDependencies": {
    "mocha": "^7.1.2",
    "pkg": "^4.3.5"
  },
  "pkg": {
    "targets": [
      "node8-alpine-x64",
      "node8-linux-x64",
      "node8-macos-x64",
      "node8-win-x64"
    ]
  }
}
